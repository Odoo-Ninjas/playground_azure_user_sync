<odoo>
	<data noupdate='0'>

		<record id="view_zbs_wiz_fileupload_form" model="ir.ui.view">
			<field name="name">zbs.wiz.fileupload form</field>
			<field name="model">zbs.wiz.fileupload</field>
			<field name="arch" type="xml">
				<form string="">
					<header></header>
					<sheet>
						<group string="Upload" col="4">
							<field name="filecontent" filename="filename"/>
							<field name="filename" invisible="1"/>
						</group>
					</sheet>
					<footer>
						<button name="ok" string="OK" type="object" />
						<button special="cancel" string="Cancel" class="oe_link" />
					</footer>
				</form>
			</field>
		</record>


	</data>
</odoo>
