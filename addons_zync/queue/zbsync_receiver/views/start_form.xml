<odoo>
    <data noupdate='0'>
        <record id="start_form" model="ir.ui.view">
            <field name="inherit_id" ref="zbsync.start_form"/>
            <field name="name">zbs.start form</field>
            <field name="model">zbs.start</field>
            <field name="arch" type="xml">
                <group position="after">
                    <group position="inside">
                        <group>
                            <field name="identifier"/>
                            <field name="url"/>
                            <field name="authorizer_id"/>
                        </group>
                        <group>
                            <div>
                        Example to upload json data:
                        requests.post("../receiver", json={})
                            </div>
                        </group>
                    </group>
                </group>
            </field>
        </record>
    </data>
</odoo>
